{
  "name": "shell",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "cypress-jira:integration": "node jira.js",
    "cypress-local:open": "concurrently '\"yarn run start\"' '\"yarn cypress-test:local\"' -k -s first",
    "cypress-test-coverage:local": "concurrently \"'yarn run e2e'\" \"'yarn cypress:open'\" -k -s first",
    "cypress-test:local": "wait-on http://localhost:5000 && yarn run cypress open --config-file projects/shell/cypress/cypress.config.ts",
    "cypress:open": "yarn run cypress open",
    "cypress:run": "yarn run cypress run --config-file projects/shell/cypress/cypress.config.ts",
    "cypress:test": "wait-on http://localhost:5000 && yarn cypress-jira:integration",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "start": "ng serve",
    "build": "ng build --base-href mfe/shell/",
    "test": "jest --coverage",
    "watch": "ng build --watch --configuration development",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
    "dependencies": {
      "@angular-architects/module-federation": "~15.0.0", 
      "@angular/animations": "^15.0.0", 
      "@angular/common": "^15.0.0", 
      "@angular/compiler": "^15.0.0", 
      "@angular/core": "^15.0.0", 
      "@angular/elements": "^15.0.0", 
      "@angular/forms": "^15.0.0", 
      "@angular/platform-browser": "^15.0.0", 
      "@angular/platform-browser-dynamic": "^15.0.0", 
      "@angular/router": "^15.0.0", 
      "@cypress/code-coverage": "^4.0.0",
      "@ib-ng/schematics": "^1.1.0-alpha.0",
      "concurrently": "^7.3.0",
      "cypress": "~10.3.2", 
      "istanbul-instrumenter-loader": "^4.0.0", 
      "jira.js": "^3.0.0", 
      "jquery": "^3.6.1",
      "jwt-decode": "^3.1.2",
      "rxjs": "~7.5.2", 
      "sass": "^1.54.10", 
      "tm4j-cypress-integration": "^0.0.6", 
      "tslib": "^2.3.1", 
      "zone.js": "~0.11.6"
    },
    "devDependencies": {
      "@angular-builders/jest": "~14.0.4", 
      "@angular-devkit/build-angular": "^15.0.0",
      "@angular-eslint/builder": "15.0.0",
      "@angular-eslint/eslint-plugin": "15.0.0",
      "@angular-eslint/eslint-plugin-template": "15.0.0",
      "@angular-eslint/schematics": "15.0.0",
      "@angular-eslint/template-parser": "15.0.0",
      "@angular/cli": "~15.0.0",
      "@angular/compiler-cli": "^15.0.0",
      "@ib-ng-utils/infra": "~2.1.0",
      "@types/jest": "~28.1.6",
      "@typescript-eslint/eslint-plugin": "5.34.0",
      "@typescript-eslint/parser": "5.29.0",
      "babel-jest": "~28.1.3",
      "eslint": "^8.18.0",
      "jest": "~28.1.3",
      "jest-preset-angular": "~12.2.0",
      "ngx-build-plus": "^15.0.0",
      "typescript": "~4.8.2"
    }
  
}
